autoscaling:
  horizontal:
    grist:
      enabled: false
      minReplicas: 1
      maxReplicas: 3
      targetCPU: 75
      targetMemory: ""
    keycloak:
      enabled: true
      minReplicas: 2
      maxReplicas: 3
      targetCPU: 75
      targetMemory: ""
  vertical:
    synapse:
        master:
          enabled: false
          annotations: {}
          controlledResources: []
          maxAllowed: {}
          minAllowed: {}
          updatePolicy:
            updateMode: Auto
